package com.adenon.sp.configuration;

import com.adenon.sp.administration.annotate.MBean;

@MBean(parent = Bean2.class, subLocation = "bean3=Third Bean", id = "id")
public class Bean3 {

    private String id   = "my id";
    private String none = "";

    public void setId(String id) {
        this.id = id;
    }

    public String getId() {
        return this.id;
    }

    public void setNone(String none) {
        this.none = none;
    }

    public String getNone() {
        return this.none;
    }

}
